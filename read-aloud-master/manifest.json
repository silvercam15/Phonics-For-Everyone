{
	"manifest_version": 2,

	"name": "Phonics For Everyone",
	"short_name": "Phonics",
	"description": "Provides phonetic pronunciation of text on a webpage.",
	"version": "0.5.1",
	"default_locale": "en",

	"browser_action": {
		"default_icon": "img/icon.png",
		"default_popup": "popup.html"
	},
	"icons": {
		"16": "img/icon.png",
		"48": "img/icon.png",
		"128": "img/icon.png"
	},
	"permissions": [
		"activeTab",
		"tts",
		"storage",
		"ttsEngine",
		"contextMenus",
		"https://support.lsdsoftware.com/"
	],
	"content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
	"web_accessible_resources": [
		"js/pdf.worker.js"
	],
	"background": {
		"scripts": [
			"js/es6-promise.auto.min.js",
			"js/defaults.js",
			"js/speech.js",
			"js/document.js",
			"js/googletr_tts.js",
			"js/events.js"
		],
		"persistent": false
	},
	"options_page": "options.html",
	"commands": {
		"_execute_browser_action": {
			"suggested_key": {
				"default": "Alt+R"
			}
		}
	},

